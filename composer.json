{
	"name": "framework/cache",
	"type": "library",
	"description": "The Cache package",
	"keywords": [
		"cache",
		"files",
		"memcached",
		"redis",
		"igbinary",
		"json",
		"msgpack"
	],
	"homepage": "https://gitlab.com/the-framework/cache",
	"license": "proprietary",
	"authors": [
		{
			"name": "Natan Felles",
			"email": "natanfelles@gmail.com"
		}
	],
	"require": {
		"php": ">=7.3",
		"ext-igbinary": "*",
		"ext-json": "*",
		"ext-memcached": "*",
		"ext-msgpack": "*",
		"ext-redis": "*"
	},
	"require-dev": {
		"ext-xdebug": "*",
		"framework/coding-standard": "dev-master",
		"localheinz/composer-normalize": "^1.2",
		"phpmd/phpmd": "^2.6",
		"phpstan/phpstan": "^0.11",
		"phpunit/phpunit": "^8.2"
	},
	"autoload": {
		"psr-4": {
			"Framework\\Cache\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\Cache\\": "tests/"
		}
	},
	"repositories": [
		{
			"type": "composer",
			"url": "https://the-framework.gitlab.io/satis/"
		}
	],
	"minimum-stability": "dev",
	"prefer-stable": true,
	"scripts": {
		"post-install-cmd": [
			".hooks/init"
		],
		"post-update-cmd": [
			"composer dump-autoload"
		]
	},
	"support": {
		"email": "natanfelles@gmail.com",
		"issues": "https://gitlab.com/the-framework/cache/issues",
		"wiki": "https://gitlab.com/the-framework/packages/cache",
		"source": "https://gitlab.com/the-framework/cache",
		"docs": "https://the-framework.gitlab.io/the-framework/packages/cache/api"
	}
}
